{
	"apiVersion": "v1",
	"kind": "Pod",
	"metadata": {
		"name": "${APP_NAME}",
		"labels": {
			"component": "${APP_NAME}"
		},
		"annotations": {
			"publicPaths": "5432:/postgres",
			"projectcalico.org/policy": "allow tcp"
		}
	},
	"spec": {
		"imagePullSecrets": [{
			"name": "ecr-key"
		}],
		"containers": [{
			"name": "${APP_NAME}",
			"image": "${IMAGE_URL}:latest",
			"ports": [{
				"containerPort": 5432 
			}]
		}]
	}
}
